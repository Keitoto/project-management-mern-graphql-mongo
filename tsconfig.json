{
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    // "moduleResolution": "nodenext",
    "outDir": "dist",
    // "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "noEmit": true,
    "paths": {
      "@/*": ["server/*"]
    }
  },
  "include": ["server/**/*.ts"],
  "exclude": ["node_modules", "client"]
}
